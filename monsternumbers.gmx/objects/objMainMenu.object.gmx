var title_x = room_width / 2;
var title_y_begin = -50;
var title_y_end = room_height / 2.5;

var button_x = room_width / 2;
var button_y_begin = room_height + 100;
var button_scale = 0.3;
var button_vgap = 50;

var start_button_y_end = title_y_end + 100;
var howto_button_y_end = start_button_y_end + button_vgap;
var quit_button_y_end = howto_button_y_end + button_vgap;

// create title
draw_set_font(ftBig);
sprite_index = sprite_from_text("Monster Numbers", 0.7);
x = title_x;
y = title_y_begin;

// create buttons
var start_button_sprite = sprite_from_text("Free", button_scale);
start_button = create_button(button_x/2, button_y_begin, start_button_sprite); 
var time_button_sprite = sprite_from_text("Timed", button_scale);
time_button = create_button(button_x*3/2, button_y_begin, time_button_sprite); 

var filler_sprite = sprite_from_text("|", button_scale);
filler_button = create_button(button_x, button_y_begin, filler_sprite); 

var howto_button_sprite = sprite_from_text("How to Play", button_scale);
howto_button = create_button(button_x, button_y_begin, howto_button_sprite);

var quit_button_sprite = sprite_from_text("Quit", button_scale);
quit_button = create_button(button_x, button_y_begin, quit_button_sprite);

// bring ui into view
var tween_title = TweenCreate(id, true);
TweenPlayOnce(tween_title, y__, title_y_begin, title_y_end, 0.7, EaseOutBack);

var tween_start_button = TweenCreate(start_button, true);
TweenPlayOnceDelay(tween_start_button, y__, button_y_begin, start_button_y_end, 0.7, EaseOutBack, 0.5);
var tween_filler_button = TweenCreate(filler_button, true);
TweenPlayOnceDelay(tween_filler_button, y__, button_y_begin, start_button_y_end, 0.7, EaseOutBack, 0.5);
var tween_time_button = TweenCreate(time_button, true);
TweenPlayOnceDelay(tween_time_button, y__, button_y_begin, start_button_y_end, 0.7, EaseOutBack, 0.5);

var tween_howto_button = TweenCreate(howto_button, true);
TweenPlayOnceDelay(tween_howto_button, y__, button_y_begin, howto_button_y_end, 0.7, EaseOutBack, 1.0);

var tween_quit_button = TweenCreate(quit_button, true);
TweenOnFinishAdd(tween_quit_button, id, menu_activate);
TweenPlayOnceDelay(tween_quit_button, y__, button_y_begin, quit_button_y_end, 0.7, EaseOutBack, 1.5);
